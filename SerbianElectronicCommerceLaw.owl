        <SerbianLaw:brojStava rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">11</SerbianLaw:brojStava>
        <SerbianLaw:tekstStava rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Meru iz stava 10. ovog člana može opozvati ili ukinuti nadležno ministarstvo po službenoj dužnosti nakon prestanka razloga zbog kojih je mera određena.</SerbianLaw:tekstStava>
    </owl:NamedIndividual>
    


    <!-- https://github.com/Sale1996/Legal-QA-Bot/blob/master/SerbianElectronicCommerceLaw.owl#clan5astav12 -->

    <owl:NamedIndividual rdf:about="https://github.com/Sale1996/Legal-QA-Bot/blob/master/SerbianElectronicCommerceLaw.owl#clan5astav12">
