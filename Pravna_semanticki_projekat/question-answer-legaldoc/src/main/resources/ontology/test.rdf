        <SerbianLaw:opisIzraza rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Potrošač je fizičko lice koje kupuje robu, odnosno usluge radi zadovoljavanja ličnih potreba ili potreba domaćinstva.</SerbianLaw:opisIzraza>
    </owl:NamedIndividual>
    


    <!-- https://github.com/Sale1996/Legal-QA-Bot/blob/master/SerbianElectronicCommerceLaw.owl#prekogranično_pružanje_usluga -->

    <owl:NamedIndividual rdf:about="https://github.com/Sale1996/Legal-QA-Bot/blob/master/SerbianElectronicCommerceLaw.owl#prekogranično_pružanje_usluga">
