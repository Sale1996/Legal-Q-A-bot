    </owl:NamedIndividual>
    


    <!-- https://github.com/Sale1996/Legal-QA-Bot/blob/master/SerbianElectronicCommerceLaw.owl#clan5astav11 -->

    <owl:NamedIndividual rdf:about="https://github.com/Sale1996/Legal-QA-Bot/blob/master/SerbianElectronicCommerceLaw.owl#clan5astav11">
        <rdf:type rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Norm"/>
        <SerbianLaw:brojStava rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">11</SerbianLaw:brojStava>
        <SerbianLaw:tekstStava rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Meru iz stava 10. ovog člana može opozvati ili ukinuti nadležno ministarstvo po službenoj dužnosti nakon prestanka razloga zbog kojih je mera određena.</SerbianLaw:tekstStava>
        <SerbianLaw:XML_URI rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://github.com/Sale1996/Legal-QA-Bot/blob/master/Zakon_o_elektronskoj_trgovini.xml#art_5a__para_11</SerbianLaw:XML_URI>
