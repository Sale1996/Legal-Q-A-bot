        <rdf:type rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Norm"/>
        <SerbianLaw:brojTacke rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">1</SerbianLaw:brojTacke>
        <SerbianLaw:tekstTacke rdf:datatype="http://www.w3.org/2001/XMLSchema#string">o nameri izricanja mere iz stava 3. ovog člana obavesti Evropsku komisiju i nadležni organ zemlje članice Evropske unije u kojoj pružalac usluge ima sedište;</SerbianLaw:tekstTacke>
        <SerbianLaw:XML_URI rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://github.com/Sale1996/Legal-QA-Bot/blob/master/Zakon_o_elektronskoj_trgovini.xml#art_5a__para_4__point_1</SerbianLaw:XML_URI>
