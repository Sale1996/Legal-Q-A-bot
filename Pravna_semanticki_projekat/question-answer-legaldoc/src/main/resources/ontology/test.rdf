    <!-- https://github.com/Sale1996/Legal-QA-Bot/blob/master/SerbianElectronicCommerceLaw.owl#clan5a -->

    <owl:NamedIndividual rdf:about="https://github.com/Sale1996/Legal-QA-Bot/blob/master/SerbianElectronicCommerceLaw.owl#clan5a">
        <rdf:type rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Norm"/>
