    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#application -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#application">
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Application_Date"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#duration -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#duration">
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Suspension"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Interval"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#efficacy -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#efficacy">
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Efficacy_Interval"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#final_date -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#final_date">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#temporal_relation"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#final_date_of"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Interval"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#final_date_of -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#final_date_of">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#finishes"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Interval"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#in_force -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#in_force">
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#In_Force_Interval"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#initial_date -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#initial_date">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#temporal_relation"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#initial_date_of"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Interval"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#initial_date_of -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#initial_date_of">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#starts"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Interval"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#produce_efficacy_modification -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#produce_efficacy_modification">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Integration"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Repeal"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Substitution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#produce_inforce_modification -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#produce_inforce_modification">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/expression.owl#Intention"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Integration"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Repeal"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Substitution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/expression.owl#Exception"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Annulment"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Renewal"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#produce_textual_modification -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#produce_textual_modification">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Annulment"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Renewal"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#End_in_Force"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#date -->

    <owl:DatatypeProperty rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#date">
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Annulment -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Annulment">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#In_Force_Modification"/>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Application -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Application">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_System"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When a local act applies in the local legal system normative supra-ordered such as when a region applies an EU directive or a decree it applies the normative contant in some act</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Application_Date -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Application_Date">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Dynamic_Temporal_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The date when a modification is applied to the destination legislative document. The date can be instantaneous, in the future or in the past. Normally this date coincides with the date of efficacy of the text in which the modification takes place.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Application_Interval -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Application_Interval">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Dynamic_Temporal_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Interval"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is the interval during which a normative document, or fragment, produces the consequences that the normative provision establishes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Delivery_Date -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Delivery_Date">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Static_Temporal_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The date when the competent authorities finalise the document by affixing their signatures to it (e.g. promulgation by the president, signature by the queen etc.) This is the date shown on the document itself: it is an objective element and clearly identifyable.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Deregulation -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Deregulation">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_System"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When a part of some primary legislative source is delegated to some secondary legislative source (e.g. from act to regulation). </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Dynamic_Temporal_Entity -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Dynamic_Temporal_Entity">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Temporal_Occurrence"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A dynamic temporal entity represents the norm in its evolution over time.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Efficacy_Interval -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Efficacy_Interval">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Dynamic_Temporal_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Interval"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The period during which a normative fragment may be either operative or inoperative, by explicit provision of the document itself. The document is said to be in a period of operation, or enforceability or efficacy, when it may or must be applied or enforced.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Efficacy_Modification -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Efficacy_Modification">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Temporal_Modification"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Modifies the efficacy period of some norm</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#End_efficacy -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#End_efficacy">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Efficacy_Modification"/>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#End_in_Force -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#End_in_Force">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#In_Force_Modification"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When an act states the end of the period of enactment, and includes an implicit repealing of the text.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Enter_in_Force_Date -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Enter_in_Force_Date">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Static_Temporal_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The date when a dnormative document becomes law and enters the legal system for the first time. This is the moment in the doucment&apos;s history starting from which the document can be amended, its applicability assessed, and its manner of producing an effect determined.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Exception -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Exception">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_Scope"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Restricts the scope of normative content to some particular jurisdiction</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Existence_Date -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Existence_Date">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Moment"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The date when the formal act by which a legislative body freezes the document into its final form, it is the time when the document can be said to have begun its existence.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Extension -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Extension">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_Scope"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extends the scope of normative content to some particular jurisdiction</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#In_Force_Interval -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#In_Force_Interval">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Dynamic_Temporal_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time.owl#Interval"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The period during which a normative fragment belongs to the normative system. The period of force for each fragment may change over time as a function of the modifications the document goes through.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#In_Force_Modification -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#In_Force_Modification">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Temporal_Modification"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Modifies the period of being in force of some norm</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Integration -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Integration">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Textual_Modification"/>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Interpretation -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Interpretation">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_Meaning"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Modification of the interpretation of normative content by the author or some superior actor</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/legal-action.owl#Public_Act"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See ESTRELLA deliverable 3.2</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_Meaning -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_Meaning">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Semantic_Annotation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A modification that changes the meaning of some provision without changing its literal text</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_Scope -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_Scope">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Semantic_Annotation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See ESTRELLA deliverable 3.2</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_System -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/time-modification.owl#Modification_of_System">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/time-modification.owl#Semantic_Annotation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See ESTRELLA deliverable 3.2</rdfs:comment>
